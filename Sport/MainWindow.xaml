<Window x:Class="Sport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sport"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="Sport" MinHeight="450" MinWidth="1200" MaxWidth= "1250" MaxHeight="700" Icon="Resources/photo_2023-10-11_14-55-39.png">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60*"/>
            <RowDefinition Height="349*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#FFA500">
            <Image Width="45" Height="45" HorizontalAlignment="Left" Source="Resources/photo_2023-10-11_14-55-39.png" Margin="5"/>
            <Button x:Name="btnBack" HorizontalAlignment="Right" Content="Назад" Click="btnBack_Click"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>
